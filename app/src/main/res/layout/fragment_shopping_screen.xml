<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0072D7">

    <LinearLayout
        android:layout_width="121dp"
        android:layout_height="619dp"
        android:layout_marginTop="160dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.687"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="match_parent"
            android:layout_height="97dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            android:visibility="visible"
            tools:srcCompat="@drawable/santhat"
            tools:visibility="visible" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            tools:srcCompat="@drawable/bball" />

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            tools:srcCompat="@drawable/easter" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            tools:srcCompat="@drawable/sunglasses" />

    </LinearLayout>

    <Button
        android:id="@+id/shoppingScreenToHomeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="Home"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="41dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="70dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="36dp"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="121dp"
        android:layout_height="619dp"
        android:layout_marginTop="160dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.243"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            android:visibility="visible"
            tools:srcCompat="@drawable/hat"
            tools:visibility="visible" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            tools:srcCompat="@drawable/flag" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            tools:srcCompat="@drawable/pinkboots" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            tools:srcCompat="@drawable/sneakers" />

        <ImageButton
            android:id="@+id/imageButton1"
            android:layout_width="match_parent"
            android:layout_height="104dp"
            android:background="#0072D7"
            android:scaleType="centerCrop"
            tools:srcCompat="@drawable/halloween" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>